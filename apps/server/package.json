{
  "name": "dayan_backend",
  "version": "0.0.1",
  "scripts": {
    "start": "ts-node --transpile-only src/server",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --respawn src/server",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "reset_db": "rm -rf ./data/*",
    "studio": "npx prisma studio",
    "generate": "npx prisma generate",
    "format": "npx prisma format",
    "seed": "npx prisma db seed",
    "reset": "npx prisma migrate reset --skip-seed",
    "push": "npx prisma db push",
    "ws": "open http://localhost:3000/ws"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server": "^3.9.0",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "bad-words": "^3.0.4",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.18.1",
    "graphql": "^15.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-scalars": "^1.17.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.9.1",
    "ioredis": "^5.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "make-dir": "^3.1.0",
    "node-cron": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.16",
    "tslib": "^2.4.0",
    "type-graphql": "^1.1.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/bad-words": "^3.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-upload": "^8.0.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.191",
    "@types/node-cron": "^3.0.2",
    "@types/shortid": "^0.0.29",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "prisma": "^5.15.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typegraphql-prisma": "^0.27.2",
    "typescript": "^4.7.4"
  },
  "ts-standard": {
    "ignore": [
      "global.d.ts",
      "src/generated/**/*.ts"
    ]
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
